script Route135_EventScript_SeasideCaveSign {
	lock
	msgbox(Route135_Text_SeasideCaveSign, MSGBOX_SIGN)
	release
}

text Route135_Text_SeasideCaveSign{
	format("SEASIDE CAVE\pA refreshing breeze from the sea fills the air inside the cave.")
}

raw `
