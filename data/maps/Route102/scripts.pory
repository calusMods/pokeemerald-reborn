script Route102_OnTransition {
	call(Route102_EventScript_RandomizeEnconters)
	call_if_eq(VAR_PETALBURG_GYM_STATE, 1, Route102_EventScript_HideEncounters)
}

script Route102_OnResume {
	call_if_set(FLAG_SYS_CTRL_OBJ_DELETE, Common_EventScript_RemoveStaticPokemonEncounter)
}

script Route102_EventScript_HideEncounters {
	setflag(FLAG_TEMP_A)
}

script Route102_EventScript_RandomizeEnconters {
	// Encounter VAR_5
	setflag(FLAG_TEMP_5)
	random(10)
	if(var(VAR_RESULT) < 7) {
		call(Route102_EventScript_RandomizeEncounterVar5)
		clearflag(FLAG_TEMP_5)
	}
	// Encounter VAR_6
	setflag(FLAG_TEMP_6)
	random(10)
	if(var(VAR_RESULT) < 8) {
		call(Route102_EventScript_RandomizeEncounterVar6)
		clearflag(FLAG_TEMP_6)
	}
	// Encounter VAR_7
	setflag(FLAG_TEMP_7)
	random(10)
	if(var(VAR_RESULT) < 8) {
		call(Route102_EventScript_RandomizeEncounterVar7)
		clearflag(FLAG_TEMP_7)
	}
	// Encounter VAR_8
	setflag(FLAG_TEMP_8)
	random(10)
	if(var(VAR_RESULT) < 9) {
		call(Route102_EventScript_RandomizeEncounterVar8)
		clearflag(FLAG_TEMP_8)
	}
	// Encounter VAR_9
	setflag(FLAG_TEMP_9)
	random(10)
	if(var(VAR_RESULT) < 7) {
		call(Route102_EventScript_RandomizeEncounterVar9)
		clearflag(FLAG_TEMP_9)
	}
	// Encounter VAR_A
	setflag(FLAG_TEMP_A)
	random(10)
	if(var(VAR_RESULT) < 8) {
		call(Route102_EventScript_RandomizeEncounterVarA)
		clearflag(FLAG_TEMP_A)
	}
	// Encounter VAR_B
